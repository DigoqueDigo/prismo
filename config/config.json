{
  "job": {
    "name": "job",
    "numjobs": 1,
    "iterations": 1e6,
    "filename": "testfile",
    "block_size": 4096,
    "limit": 65536,
    "metric": "full"
  },

  "operation": {
    "type": "constant",
    "operation": "write",
    "percentages": {
      "read": 90,
      "write": 10
    },
    "operations": [
      "read",
      "write",
      "fsync",
      "fdatasync",
      "nop"
    ],
    "barrier": [
    ]
  },

  "access": {
    "type": "sequential",
    "skew": 0.9
  },

  "generator": {
    "type": "dedup",
    "distribution": [
      {
        "percentage": 25,
        "copies": 0
      },
      {
        "percentage": 25,
        "copies": 1
      },
      {
        "percentage": 25,
        "copies": 3
      },
      {
        "percentage": 25,
        "copies": 5
      }
    ]
  },

  "engine": {
    "type": "posix",
    "openflags": [
      "O_CREAT",
      "O_RDWR"
    ],
    "entries": 128,
    "params": {
      "cq_entries": 0,
      "sq_thread_cpu": 0,
      "sq_thread_idle": 0,
      "flags": [
        "IORING_SETUP_SQPOLL"
      ]
    },
    "bdev_name": "Malloc0",
    "reactor_mask": "0xF",
    "spdk_threads": 1,
    "json_config_file": "/home/digo/Desktop/prismo/config/spdk_bdev.json"
  },

  "logging": {
    "type": "spdlog",
    "name": "prismo",
    "queue_size": 8192,
    "thread_count": 1,
    "truncate": true,
    "to_stdout": true,
    "files": [
      "./log1.log",
      "./log2.log",
      "./log3.log"
    ]
  }
}
