{
  "job": {
    "name": "job",
    "numjobs": 1,
    "filename": "testfile",
    "block_size": 4096,
    "limit": 65536,
    "iometric": "none"
  },

  "operation": {
    "type": "constant",
    "operation": "read",
    "barrier": {
      "flush": 0,
      "fsync": 0,
      "fdata_sync": 0
    }
  },

  "access": {
    "type": "zipfian",
    "skew": 0.9
  },

  "generator": {
    "type": "constant"
  },

  "engine": {
    "posix": {
      "flags": ["O_CREAT", "O_RDWR", "O_DIRECT"],
      "fsync_close": true
    }
  },

  "logging": {
    "spdlog": {
      "name": "dedisbench",
      "queue_size": 8192,
      "thread_count": 1,
      "truncate": true,
      "stdout": true,
      "files": [
        "./log1.log",
        "./log2.log",
        "./log3.log"
      ]
    }
  }
}
