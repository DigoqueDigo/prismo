{
  "job": {
    "name": "job",
    "numjobs": 1,
    "filename": "testfile",
    "block_size": 4096,
    "limit": 4096000,
    "metric": "full"
  },

  "operation": {
    "type": "constant",
    "operation": "write",
    "read_percentage": 100,
    "operations": [
      "read",
      "write",
      "write",
      "read",
      "read"
    ],
    "barrier": {
      "flush": 0,
      "fsync": 0,
      "fdata_sync": 0
    }
  },

  "access": {
    "type": "sequential",
    "skew": 0.9
  },

  "generator": {
    "type": "constant"
  },

  "engine": {
    "type": "posix",
    "flags": [
      "O_CREAT",
      "O_RDWR",
      "O_DIRECT"
    ]
  },

  "logging": {
    "type": "spdlog",
    "name": "dedisbench",
    "queue_size": 8192,
    "thread_count": 1,
    "truncate": true,
    "stdout": true,
    "files": [
      "./log1.log",
      "./log2.log",
      "./log3.log"
    ]
  }
}