{
  "job": {
    "name": "job",
    "numjobs": 1,
    "iterations": 1e5,
    "filename": "testfile",
    "block_size": 4096,
    "limit": 65536,
    "metric": "full"
  },

  "operation": {
    "type": "constant",
    "operation": "write",
    "percentages": {
      "read": 50,
      "write": 50
    },
    "operations": [
      "read",
      "write",
      "fsync",
      "fdatasync",
      "nop"
    ],
    "barrier": [
    ]
  },

  "access": {
    "type": "sequential",
    "skew": 0.9
  },

  "generator": {
    "type": "constant"
  },

  "engine": {
    "type": "aio",
    "openflags": [
      "O_CREAT",
      "O_RDWR"
    ],
    "entries": 128,
    "params": {
      "cq_entries": 0,
      "sq_thread_cpu": 0,
      "sq_thread_idle": 0,
      "flags": [
        "IORING_SETUP_SQPOLL"
      ]
    }
  },

  "logging": {
    "type": "spdlog",
    "name": "prismo",
    "queue_size": 8192,
    "thread_count": 1,
    "truncate": true,
    "to_stdout": true,
    "files": [
      "./log1.log",
      "./log2.log",
      "./log3.log"
    ]
  }
}
