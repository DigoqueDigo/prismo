{
  "job": {
    "name": "job",
    "numjobs": 1,
    "iterations": 257,
    "filename": "testfile",
    "block_size": 4096,
    "limit": 65536,
    "metric": "full"
  },

  "operation": {
    "type": "constant",
    "operation": "write",
    "read_percentage": 50,
    "operations": [
      "read",
      "write",
      "write",
      "read",
      "read"
    ],
    "barrier": {
      "fsync": 0,
      "fdatasync": 0
    }
  },

  "access": {
    "type": "sequential",
    "skew": 0.9
  },

  "generator": {
    "type": "constant"
  },

  "engine": {
    "type": "uring",
    "batch": 128,
    "entries": 128,
    "openflags": [
      "O_CREAT",
      "O_RDWR"
    ],
    "params": {
      "sq_thread_cpu": 0,
      "sq_thread_idle": 100,
      "flags": [
        "IORING_SETUP_SQPOLL",
        "IORING_SETUP_SQ_AFF"
      ]
    }
  },

  "logging": {
    "type": "spdlog",
    "name": "dedisbench",
    "queue_size": 8192,
    "thread_count": 1,
    "truncate": true,
    "stdout": true,
    "files": [
      "./log1.log",
      "./log2.log",
      "./log3.log"
    ]
  }
}
