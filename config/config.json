{
  "job": {
    "name": "job",
    "numjobs": 1,
    "iterations": 5000000,
    "filename": "testfile",
    "block_size": 4096,
    "limit": 65536,
    "metric": "none"
  },

  "operation": {
    "type": "constant",
    "operation": "nop",
    "percentages": {
      "read": 20,
      "write": 80
    },
    "operations": [
      "read",
      "write",
      "fsync",
      "fdatasync",
      "nop"
    ],
    "barrier": [
    ]
  },

  "access": {
    "type": "sequential",
    "skew": 0.9
  },

  "generator": {
    "type": "constant"
  },

  "engine": {
    "type": "posix",
    "openflags": [
      "O_CREAT",
      "O_RDWR"
    ],
    "entries": 8,
    "params": {
      "cq_entries": 0,
      "sq_thread_cpu": 0,
      "sq_thread_idle": 0,
      "flags": [
        "IORING_SETUP_SQPOLL"
      ]
    }
  },

  "logging": {
    "type": "spdlog",
    "name": "dedisbench",
    "queue_size": 8192,
    "thread_count": 1,
    "truncate": true,
    "to_stdout": true,
    "files": [
      "./log1.log",
      "./log2.log",
      "./log3.log"
    ]
  }
}
